@font-face {
	font-family: 'iconos';
	src:url('iconos/fonts/icomoon.eot');
	src:url('iconos/fonts/icomoon.eot?#iefix') format('embedded-opentype'),
		url('iconos/fonts/icomoon.woff') format('woff'),
		url('iconos/fonts/icomoon.ttf') format('truetype'),
		url('iconos/fonts/icomoon.svg#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

body
	background rgba 162, 178, 186, .70
	font-size 16px
	body-scroll none
	margin 0
	padding 0
	position fixed
	text-decoration none
	width 100%

footer
	background #494949
	bottom 0
	color white
	padding .2em 0
	position fixed
	text-align center
	width 100%

	a:hover
		color #E61C83

h1
	font-size 1em
h2
	color white		

header
	background #e4e4e1
	height 40px
	position fixed
	width 100%

	
#contacto, #logo, #redes
	display inline-block
	vertical-align top

#contacto 
	background #dbdbd5
	display inline-block
	height 40px
	margin 0 10px 0 19px
	position relative
	width 50px

	&:after
		border-bottom 40px solid transparent
		border-left 25px solid #dbdbd5
		content " "
		left 100%
		position absolute
		top 0

#logo
	background #E61C83
	height 40px
	position relative
	width 150px
	
	&:after
		border-bottom 40px solid #dbdbd5
		border-left 25px solid #e61c83
		content " "
		left 100%
		position absolute
		top 0

.contact 
	color #767676
			
	&:before
		content: "\e600"
		display inline-block
		font-family "iconos"
		font-size 28px
		margin 5px 10px

.contact:hover
	color #E61C83

.logo_head
	height 30px
	margin 5px 30px
	width 80px

#formulario-contacto
	background #dbdbd5
	border-radius 0 0 10px 0
	width 30%
