.formulario  
    color  #666666
    display inline-block
    font-family Arial 
    margin 60px 0 0
    text-align center
    width 410px 
 
form  
    margin 0 auto 
 
 
input, textarea  
    background #666666 
    border 2px solid #666 
    border-radius 10px
    color #ffffff 
    font-size 10px 
    height 27px 
    margin-top 5px 
    padding 10px 
    width 300px 

 
textarea  
    height 150px 
 
 
#submit  
    border none 
    border-radius 10%
    cursor pointer 
    display block 
    height 40px
    margin 10px auto
    width 80px 
 
